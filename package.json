{
  "name": "mini-crud",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only src/index.ts",
    "build": "rimraf dist/ && tsc --noEmitOnError --pretty  --extendedDiagnostics",
    "start": "node -r module-alias/register dist/index.js",
    "test": "jest --passWithNoTests",
    "test:docs": "swagger-cli validate src/docs/openapi.yaml",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "validate": "npm run lint && npm run type-check && npm test",
    "prisma:generate": "prisma generate",
    "generate:openapi": "npx swagger-jsdoc -d ./docs/openapi.config.js -o ./docs/openapi.yaml",
    "postinstall": "npm run generate:openapi && npm run prisma:generate",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx --max-warnings=5",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,mdx,yml,yaml}\"",
    "lint:fix": "npm run lint -- --fix",
    "type-check": "tsc --noEmit",
    "prepare": "husky install",
    "ci": "echo 'CI=true'",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "@bull-board/api": "^6.12.7",
    "@bull-board/express": "^6.12.7",
    "@prisma/client": "^5.22.0",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "chalk": "^5.6.0",
    "chalk-animation": "^2.0.3",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "dotenv-safe": "^9.1.0",
    "env-var": "^7.5.0",
    "express": "^4.21.2",
    "express-rate-limit": "^8.0.1",
    "figlet": "^1.8.2",
    "gradient-string": "^3.0.0",
    "helmet": "^8.1.0",
    "module-alias": "^2.2.3",
    "morgan": "^1.10.1",
    "prom-client": "^15.1.3",
    "rimraf": "^6.0.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "vitest": "^3.2.4",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "winston-loki": "^6.1.3",
    "winston-mongodb": "^7.0.1",
    "yamljs": "^0.3.0"
  },
  "_moduleAliases": {
    "@": "dist",
    "@config": "dist/config",
    "@services": "dist/services",
    "@middlewares": "dist/middlewares",
    "@router": "dist/router",
    "@utils": "dist/utils",
    "@controllers": "dist/controllers"
  },
  "devDependencies": {
    "@types/chalk": "^0.4.31",
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/csurf": "^1.11.5",
    "@types/dotenv-safe": "^8.1.6",
    "@types/express": "^5.0.3",
    "@types/figlet": "^1.7.0",
    "@types/jest": "^30.0.0",
    "@types/k6": "^1.2.0",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.3.0",
    "@types/supertest": "^6.0.3",
    "@types/yamljs": "^0.2.34",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-sonarjs": "^3.0.5",
    "eslint-plugin-unicorn": "^60.0.0",
    "eslint-plugin-unused-imports": "^4.2.0",
    "jest": "^30.1.1",
    "k6": "^0.0.0",
    "prettier": "^3.6.2",
    "prisma": "^5.0.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  }
}
